.date-time-field input {
	color: var(--date-picker-foreground, #000000);
	background-color: var(--input_inactive_bg_color);
	min-width: 0px;
	box-sizing: border-box;
	padding: 0.25rem 0.75rem;
	margin: 0px;
	border-radius: 3px;
	width: var(--date-input-width, 150px);
	border: none;
	box-shadow: 0 0 0 1.5px var(--input_inactive_border_color);
	transition: all 80ms cubic-bezier(0.4, 0, 0.2, 1);
}
